{
  "$schema": "https://nitro.margelo.com/nitro.schema.json",
  "cxxNamespace": ["healthkit"],
  "ios": {
    "iosModuleName": "ReactNativeHealthkit"
  },
  "android": {
    "androidCxxLibName": "ReactNativeHealthkit",
    "androidNamespace": ["healthkit"]
  },
  "autolinking": {
    "WorkoutsModule": {
      "swift": "WorkoutsModule"
    },
    "CoreModule": {
      "swift": "CoreModule"
    },
    "CharacteristicTypeModule": {
      "swift": "CharacteristicTypeModule"
    },
    "QuantityTypeModule": {
      "swift": "QuantityTypeModule"
    },
    "CategoryTypeModule": {
      "swift": "CategoryTypeModule"
    },
    "CorrelationTypeModule": {
      "swift": "CorrelationTypeModule"
    },
    "ElectrocardiogramModule": {
      "swift": "ElectrocardiogramModule"
    },
    "HeartbeatSeriesModule": {
      "swift": "HeartbeatSeriesModule"
    },
    "StateOfMindModule": {
      "swift": "StateOfMindModule"
    },
    "MedicationModule": {
      "swift": "MedicationModule"
    }
  },
  "ignorePaths": ["**/node_modules"]
}
